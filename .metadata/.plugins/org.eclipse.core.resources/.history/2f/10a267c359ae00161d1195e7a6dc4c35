package player;

public class Human extends Player{
	
	/**Constructeur de joueur qui est appelé pour créer un joueur humain*/
	public Human(String pseudo, int age) {
		super(pseudo, age);
	}
	
	public void play(){
		
	}

	@Override
	public String toString() {
		return "Human [score=" + score + ", hand=" + hand + "]";
	}

}
